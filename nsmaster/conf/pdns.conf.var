allow-axfr-ips=10.0.1.0/24
also-notify=10.0.1.5
api=yes
api-key=${NSMASTER_APIKEY}
default-soa-edit=INCREMENT-WEEKS
default-soa-mail=hostmaster.dns.test
default-soa-name=ns1.dns.test
default-ttl=60
master=yes
only-notify=
setgid=pdns
setuid=pdns
webserver=yes
webserver-address=0.0.0.0
webserver-allow-from=10.0.1.0/24

launch=gmysql
gmysql-host=dbmaster
gmysql-port=
gmysql-dbname=pdns
gmysql-user=pdns
gmysql-password=${DBMASTER_PASSWORD_pdns}
gmysql-dnssec=yes
