allow-unsigned-notify=yes
allow-unsigned-supermaster=yes
api=yes
api-key=${NSSLAVE_APIKEY}
disable-axfr=yes
setgid=pdns
setuid=pdns
slave=yes
webserver=yes
webserver-address=0.0.0.0
webserver-allow-from=10.0.1.0/24

launch=gmysql
gmysql-host=dbslave
gmysql-port=
gmysql-dbname=pdns
gmysql-user=pdns
gmysql-password=${DBSLAVE_PASSWORD_pdns}
gmysql-dnssec=yes
